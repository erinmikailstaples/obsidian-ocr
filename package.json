{
  "name": "obsidian-ocr-plugin",
  "version": "1.0.0",
  "description": "Convert handwritten text images to Obsidian notes using OCR",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ocr"
  ],
  "author": "Erin Mikail Staples",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/erinmikailstaples/obsidian-ocr.git"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "builtin-modules": "^4.0.0",
    "esbuild": "^0.24.2",
    "obsidian": "^1.7.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "tesseract.js": "^5.1.1"
  }
}
